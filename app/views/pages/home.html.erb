<div class="px-4 py-5 my-5 text-center">
  <%= image_tag "logo.jpg", class: "d-block mx-auto mb-4", width: "72"%>
  <h1 class="display-5 fw-bold">Starent</h1>
  <div class="col-lg-6 mx-auto">
    <p class="lead mb-4">The best website to spend your day with your prefered star, scroll down üëáüèº and see all the available stars to spend a day with you.</p>
    <div class="d-grid gap-2 d-sm-flex justify-content-sm-center">
      <button type="button" class="btn btn-primary btn-lg px-4 gap-3">Book a star</button>
    </div>
  </div>
</div>

<div class="container">
  <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
    <% @star.each do |s| %>
      <div class="col">
        <div class="card shadow-sm">
          <svg class="bd-placeholder-img card-img-top" width="100%" height="225" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice" focusable="false"><title>Placeholder</title><rect width="100%" height="100%" fill="#55595c"></rect><text x="50%" y="50%" fill="#eceeef" dy=".3em">Thumbnail</text></svg>

          <div class="card-body">
            <p class="card-text"><%= s.name %></p>
            <div class="d-flex justify-content-between align-items-center">
              <div class="btn-group">

              </div>
              <small class="text-muted">9 mins</small>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

